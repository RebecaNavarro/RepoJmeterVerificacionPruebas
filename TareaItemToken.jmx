          <stringProp name="BeanShellSampler.query">import org.apache.commons.codec.binary.Base64;

String email = &quot;tareajmeter@2024.com&quot;;
String password = &quot;12345&quot;;

String auth = email + &quot;:&quot; + password;

byte[] base64Auth = Base64.encodeBase64(auth.getBytes());

vars.put(&quot;AUTH_TOKEN&quot;, new String(base64Auth));

log.info(&quot;Encoded Base64 Auth Token: &quot; + new String(base64Auth));</stringProp>
